{
    {
        {
            {
                var sera = 'Será??'
            }
        }
    }
}
console.log(sera)
//variaveis normais podem ser chamadas fora de seus blocos

function teste() {
    var local = 123
    console.log(local)
}

teste()

//se a variavel for criada dentro de uma função
//ela não poderá ser chamada fora dela