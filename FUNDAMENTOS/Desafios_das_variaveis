//Desafio do calculo de área
const PI = 3.14;
let raio = 10;
let resultado = PI * (raio * raio);

console.log("O Resultado da Circunferência é: "
 + resultado + " m²");

//Desafio da Troca 
let a = 7;
let b = 94;
let substituta = 0;

substituta = a
a = b;
b = substituta;

console.log(a);
console.log(b);

//forma simplificada de trocar valores
let nomeA = 'Carlos';
let nomeB = 'Maria';

[nomeA, nomeB] = [nomeB, nomeA];

console.log(nomeA);
console.log(nomeB);